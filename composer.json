{
    "name": "uran1980/yii2-advanced-template-custom",
    "description": "Improved Yii 2 Advanced Application Template By Nenad Zivkovic And customized by Uran1980",
    "keywords": ["yii2", "framework", "advanced", "improved", "application template", "nenad", "uran1980"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "tutorial": "http://www.freetuts.org/tutorial/view?id=6",
        "source": "https://github.com/uran1980/yii2-advanced-template-custom"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-redis": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-imagine": "*",

        "nenad/yii2-password-strength": "*",
        "mihaildev/yii2-ckeditor": "*",

        "rmrevin/yii2-fontawesome": "*",
        "rmrevin/yii2-minify-view": "*",
        "codemix/yii2-localeurls": "*",
        "vova07/yii2-fileapi-widget": "*",
        "danielstjules/stringy": "*",
        "uran1980/yii2-translate-panel": "*",
        "uran1980/yii2-scroll-to-top": "*",
        "uran1980/yii2-pace-widget": "*",

        "bower-asset/awesome-bootstrap-checkbox": "*",
        "bower-asset/bootstrap-sass-official": "*",
        "bower-asset/compass-mixins": "*",
        "bower-asset/datetimepicker": "*",
        "bower-asset/fancybox": "*",
        "bower-asset/formValidation": "*",
        "bower-asset/holderjs": "*",
        "bower-asset/jquery-form": "*",
        "bower-asset/jquery.easing": "*",
        "bower-asset/jquery-cookie": "*",
        "bower-asset/jquery-mousewheel": "*",
        "bower-asset/jquery-ui": "*",
        "bower-asset/jqueryui-timepicker-addon": "*",
        "bower-asset/ladda-bootstrap": "*",
        "bower-asset/perfect-scrollbar": "*",
        "bower-asset/spin.js": "*",
        "bower-asset/stitch-css": "*",
        "bower-asset/scrollMonitor": "*",
        "bower-asset/x-editable": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/codeception": "*",
        "codeception/phpbuiltinserver": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
